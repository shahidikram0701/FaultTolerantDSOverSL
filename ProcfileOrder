# Use goreman to run `go get github.com/mattn/goreman`

order-1: ./scalog order --config=.scalog.yaml --oid=0
order-2: ./scalog order --config=.scalog.yaml --oid=1
order-3: ./scalog order --config=.scalog.yaml --oid=2
data-0-0: ./scalog data --config=.scalog.yaml --sid=0 --rid=0
data-0-1: ./scalog data --config=.scalog.yaml --sid=0 --rid=1
data-1-0: ./scalog data --config=.scalog.yaml --sid=1 --rid=0
data-1-1: ./scalog data --config=.scalog.yaml --sid=1 --rid=1
disc: ./scalog discovery --config=.scalog.yaml
